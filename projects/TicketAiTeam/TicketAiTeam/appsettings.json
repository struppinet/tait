{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning",
      "TicketAiTeam": "Trace"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "ColorBehavior": "Enabled",
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff zzzz] "
      }
    }
  },
  "AllowedHosts": "*"
}
